from secrets import choice
num1=eval(input("enter number:"))
num2=eval(input("enter number2:"))
print("1)Add")
print("2)sub")
print("3)mul")
print("4)div")
choice=int(input("please enter choice:"))
if choice==1:
    print("Add", num1,"and", num2,"is",num1+num2)
if choice==2:
    print("Add", num1,"and", num2,"is",num1-num2)
if choice==3:
    print("Add", num1,"and", num2,"is",num1*num2)
if choice==4: 
    print("Add", num1,"and", num2,"is",num1//num2)
    print("sorry")
